<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.capstone">

    <!--권한-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" /><!--카카오-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/><!--카카오-->
    <!--카메라 권한-->
    <uses-permission android:name="android.permission.CAMERA"/>
    <!--저장소 쓰기 권한-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>    <!--저장소 읽기 권한-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <!--Piechart-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="RM"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Capstone"
        android:name=".MyApplication"
        tools:targetApi="31">
        <provider
            android:authorities="com.example.capstone.fileprovider"
            android:name="androidx.core.content.FileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths"></meta-data>
        </provider>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirBtnActivity"/>
        <activity android:name=".SearchActivity"/>
        <activity android:name=".SearchBtnActivity"/>
        <activity android:name=".ChatbotActivity"/>
        <activity android:name=".InfoWritingActivity"/>
        <activity android:name=".InfoPicActivity"/>
        <activity android:name=".InfoPic2Activity"/>
        <activity android:name=".AIActivity"/>
        <activity android:name=".AIMusicalActivity"/>
        <activity android:name=".AISeatActivity"/>
        <activity android:name=".AILocActivity"/>
        <activity android:name=".TheaterActivity"/>
        <activity android:name=".Theater2Activity"/>
        <activity android:name=".BoardActivity"/>
        <activity android:name=".BoardWriteActivity"/>
        <activity android:name=".SettingActivity"/>
        <activity android:name=".AddInfoActivity"/>
        <activity android:name=".EditActivity"/>
        <!--인증-->
        <activity android:name=".AuthActivity"
            android:label="Authentication"/>
        <!--카카오 -->
        <meta-data
            android:name="com.kakao.vectormap.APP_KEY"
            android:value="b599549a38df11456b711bd741bd37f4"/>
    </application>


</manifest>